# DeepSeek API ключ
# Получить можно на https://platform.deepseek.com/api_keys
DEEPSEEK_API_KEY=sk-your-deepseek-api-key-here

# Telegram Bot Token (для Telegram интеграции)
# Получить можно у @BotFather в Telegram
TELEGRAM_BOT_TOKEN=your-telegram-bot-token-here

# Настройки для DeepSeek
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_BASE_URL=https://api.deepseek.com

# Модель для эмбеддингов
EMBEDDINGS_MODEL=paraphrase-multilingual-mpnet-base-v2
RETRIEVER_K=5

# Файлы данных (опционально, есть значения по умолчанию)
JSON_DATA_FILE=itmo_programs_full.json

# Логирование (опционально)
LOG_LEVEL=INFO
